<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.mydroidcafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_my_app_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyDroidCafe">
        <activity android:name=".StoreActivity"></activity>
        <activity android:name=".SundaeActivity" />
        <activity android:name=".CakeActivity" />
        <activity android:name=".FroyoActivity" />
        <activity android:name=".IceCreamActivity" />
        <activity android:name=".DonutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>